        VET[2] <- 26
        VET[3] <- 74
        VET[4] <- 56
        VET[5] <- 49
        VET[6] <- 23
        VET[7] <- 14
        VET[8] <- 75
      Escreval (" ")
      Escreval ("SEQUÊNCIA: ")
      Escreval (" ")
      Escreval ("[", VET[1], " ]", "[", VET[2], " ]", "[", VET[3], " ]", "[", VET[4], " ]", "[", VET[5], " ]", "[", VET[6], " ]", "[", VET[7], " ]", "[", VET[8], " ]")
